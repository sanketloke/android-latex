<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_height="wrap_content"
	android:layout_width="fill_parent" android:background="@drawable/login_down">
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<TextView android:text="@string/projectname"
			android:layout_height="fill_parent" android:layout_width="0dp"
			android:layout_weight="1" android:gravity="center_vertical" />
		<EditText android:id="@+projectlist/add"
			android:layout_height="wrap_content" android:layout_width="0dp"
			android:layout_weight="1" />
	</LinearLayout>
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<TextView android:text=" " android:layout_height="fill_parent"
			android:layout_width="0dp" android:layout_weight="1" android:gravity="center_vertical" />
		<Button android:id="@+projectlist/validadd"
			android:layout_height="wrap_content" android:layout_width="0dp"
			android:layout_weight="1" android:text="Valider" />
	</LinearLayout>

</LinearLayout>